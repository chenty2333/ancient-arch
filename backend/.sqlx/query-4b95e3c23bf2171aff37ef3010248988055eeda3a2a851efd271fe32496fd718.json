  "db_name": "PostgreSQL",
  "query": "\n        SELECT \n            id, user_id, title, content, \n            created_at,\n            updated_at,\n            deleted_at,\n            likes_count, comments_count, favorites_count\n        FROM posts\n        WHERE deleted_at IS NULL\n          AND ($1::TIMESTAMPTZ IS NULL OR created_at < $1)\n        ORDER BY created_at DESC\n        LIMIT $2\n        ",
